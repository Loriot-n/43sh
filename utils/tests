[01]
NAME="Path 1"
sETUP="PATH='/usr/bin:/bin' ; touch file$$"
CLEAN="rm -f file$$"
TESTS=
  echo 'ls -la /bin .'
  echo 'echo $?'
[01-END]

[02]
NAME="Path 2"
SETUP="PATH='/bin' ; touch file$$"
CLEAN="rm -f file$$"
TESTS=
  echo 'ls -la /bin .'
  echo 'echo $?'
[02-END]

[03]
NAME="Path 3"
SETUP="PATH=':/usr/bin:/bin:' ; touch file$$"
CLEAN="rm -f file$$"
TESTS=
  echo 'ls -la /bin .'
  echo 'echo $?'
[03-END]

[04]
NAME="Path 4"
SETUP="touch file$$;PATH=''"
CLEAN="/bin/rm -f file$$"
TESTS=
  echo 'ls -la /bin .'
  echo 'echo $?'
[04-END]

[05]
NAME="Path 5"
SETUP="PATH='/tmp:/root/:/bin' ; /usr/bin/touch file$$"
CLEAN="/bin/rm -f file$$"
TESTS=
  echo 'ls -la /bin .'
  echo 'echo $?'
[05-END]

[06]
NAME="Path 6"
SETUP="PATH=' ' ; /usr/bin/touch file$$"
CLEAN="/bin/rm -f file$$"
TESTS=
  echo 'ls -la /bin .'
  echo 'echo $?'
[06-END]

[07]
NAME="Path 7"
SETUP="PATH='coucou' ; /usr/bin/touch file$$"
CLEAN="/bin/rm -f file$$"
TESTS=
  echo 'ls -la /bin .'
  echo 'echo $?'
[07-END]

[08]
NAME="Path 8"
SETUP="PATH='/' ; /usr/bin/touch file$$"
CLEAN="/bin/rm -f file$$"
TESTS=
  echo 'ls -la /bin .'
  echo 'echo $?'
[08-END]

[09]
NAME="Path 9"
SETUP="PATH='.mybin' ; /usr/bin/touch file$$ ; /bin/mkdir .mybin ; /bin/cp /bin/ls .mybin/"
CLEAN="/bin/rm -f file$$ ; /bin/rm -Rf .mybin"
TESTS=
  echo 'ls -la /bin .'
  echo 'echo $?'
[09-END]

[10]
NAME="Path 10"
SETUP="unset PATH ; /usr/bin/touch file$$"
CLEAN="/bin/rm -f file$$"
TESTS=
  echo 'ls -la /bin .'
  echo 'echo $?'
[10-END]


[11]
NAME="Just a pwd"
SETUP=""
CLEAN=""
TESTS=
  echo 'pwd'
  echo 'echo $?'
[11-END]

[12]
NAME="Pipe with builtin"
SETUP="OLDPATH=$PATH ; PATH='/toto'"
CLEAN="PATH=$OLDPATH"
TESTS=
  echo 'setenv | /bin/grep PATH'
  echo 'echo $?'
[12-END]

[13]
NAME="Check exec"
SETUP="mkdir bin ; cp /bin/ls bin/cat"
CLEAN="rm -Rf bin"
TESTS=
  echo 'pwd ; cd bin ; ls -l'
  echo 'cd ..'
  echo 'bin/cat'
  sleep 0.5
  echo 'pwd'
  echo 'echo $?'
[13-END]

[14]
NAME="Check exit"
SETUP=""
CLEAN=""
TESTS=
  echo 'exit 25'
  echo 'echo $?'
[14-END]

[15]
NAME="Check non existant bin"
SETUP="PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo 'coucou'
  echo 'echo $?'
[15-END]

[16]
NAME="setenv and unsetenv"
SETUP="/usr/bin/env > .lstenv ; while read varenv ; do unset $varenv ; done < <(/bin/cat .lstenv | /usr/bin/cut -f1 -d=) ; export PATH=/bin:/usr/bin ; export _=ls ; export LS_COLORS=RED ; export MYBG=1 ; export LVL=42 ; export NOTE=0"
CLEAN="while read varenv ; do export "$varenv" 2>/dev/null ; done  < <(/bin/cat .lstenv) ; /bin/rm -f .lstenv"
TESTS=
  echo 'unsetenv HOSTTYPE VENDOR OSTYPE MACHTYPE SHLVL PWD LOGNAME USER GROUP HOST REMOTEHOST'
  echo 'unsetenv _'
  echo 'unsetenv LS_COLORS'
  echo 'unsetenv NOTE'
  echo 'setenv toto'
  echo 'setenv tata coucou'
  echo 'env'
  echo '/bin/env'
  echo 'setenv titi abcd'
  echo 'unsetenv tata'
  echo 'env'
  echo 'echo $?'
[16-END]

[17]
NAME="cd"
SETUP=""
CLEAN="rm -Rf plopplop"
TESTS=
  echo "mkdir plopplop"
  echo "cd plopplop"
  echo "pwd"
  echo -e "cd -\necho KEY\npwd\ncd /\nls"
  echo "cd -"
  echo "ls"
  echo 'echo $?'
[17-END]

[18]
NAME="cd and error messages"
SETUP="mkdir testdircd ; chmod 000 testdircd"
CLEAN="chmod 755 testdircd ; rmdir testdircd"
TESTS=
  echo "cd testdircd"
  echo 'echo $?'
[18-END]

[19]
NAME="space and tab"
SETUP=""
CLEAN=""
TESTS=
  echo -e ""
  echo -e "ls"
  echo -e "ls -l"
  echo -e "ls     -l"
  echo -e "     ls     -l"
  echo -e "ls -l     "
  echo -e "    ls     -l     -a    "
  echo -e "ls\t"
  echo -e "\tls"
  echo -e "\tls\t-l"
  echo -e "\t\t    ls\t     \t-l    \t\t  \t -a    \t\t"
  echo 'echo $?'
[19-END]

[20]
NAME="separator ;"
SETUP=""
CLEAN=""
TESTS=
  echo -e "    \t  ls  -l;echo toto  ;  ls\t;\techo plop"
[20-END]

[21]
NAME="simple pipe"
SETUP=""
CLEAN=""
TESTS=
  echo -e "ls /dev | grep tty"
  echo 'echo $?'
[21-END]

[22]
NAME="pipe"
SETUP=""
CLEAN=""
TESTS=
  echo -e "ls | grep plop"
  echo -e "\t ls   -l\t/dev|\tgrep tty"
  echo 'echo $?'
[22-END]

[23]
NAME="multipipe"
SETUP=""
CLEAN=""
TESTS=
  echo -e "ls /dev | cat  | tr y Y | grep ttY | cat"
  echo 'echo $?'
[23-END]

[24]
NAME="multipipe and FDMAX"
SETUP="ulimit -n 21"
CLEAN=""
TESTS=
  echo -e "ls -lR /dev | cat  | tr y Y | grep ttY | cat | cat | cat | cat | cat | cat |cat"
  echo 'echo $?'
[24-END]

[25]
NAME="multipipe and error"
SETUP=""
CLEAN=""
TESTS=
  echo -e "ls -lR /dev | cat  | tr y Y | grep ttY | cat | cati | cat"
  echo 'echo $?'
[25-END]

[26]
NAME="output redirect"
SETUP=""
CLEAN="rm fic"
TESTS=
  echo "ls /dev > fic"
  echo "echo toto"
  echo "cat fic"
  echo 'echo $?'
[26-END]

[27]
NAME="output double-redirect"
SETUP=""
CLEAN="rm fic"
TESTS=
  echo "ls /dev > fic"
  echo "echo toto"
  echo "echo plop >> fic"
  echo "ls / >> fic"
  echo "echo titi"
  echo "cat fic"
  echo 'echo $?'
[27-END]

[28]
NAME="output redirect on bin files"
SETUP=""
CLEAN="rm myls"
TESTS=
  echo "cat /bin/ls > myls"
  echo "chmod 755 myls"
  echo "./myls"
  echo 'echo $?'
[28-END]

[29]
NAME="input redirect"
SETUP="/bin/ls /dev > fic"
CLEAN="rm fic"
TESTS=
  echo "grep tty < fic"
  echo 'echo $?'
[29-END]

[30]
NAME="input redirect, output redirect and multi pipe and ; separator"
SETUP="/bin/ls /dev > fic"
CLEAN="rm fic ficout ficout2"
TESTS=
  echo "grep tty < fic | tr y Y > ficout ; cat ficout | sed s/tt/uu/g > ficout2 ; cat ficout2 | tr u I"
  echo 'echo $?'
[30-END]

[31]
NAME="inception"
SETUP=""
CLEAN="rm result result2"
TESTS=
  echo "echo ls | /bin/tcsh > result"
  echo "echo ls | ./42sh > result2"
  echo "cat result"
  echo "cat result2"
  echo 'echo $?'
[31-END]

[32]
NAME="redirect output on input"
SETUP="echo coucou > fic"
CLEAN="rm fic"
TESTS=
  echo "cat fic >> fic"
  echo "cat fic"
  echo 'echo $?'
[32-END]

[33]
NAME="unsetenv all"
SETUP=""
CLEAN=""
TEST=
  echo "unsetenv *"
  echo "env"
  echo 'echo $?'
[33-END]

[34]
NAME="ls HOME"
SETUP=""
CLEAN=""
TEST=
  echo "ls $HOME"
  echo 'echo $?'
[34-END]

[35]
NAME="echo with cmd inside"
SETUP=""
CLEAN=""
TEST=
  echo "echo 'ls -al | grep Makefile;'
  echo 'echo $?'
[35-END]

[36]
NAME="man ls"
SETUP=""
CLEAN=""
TEST=
  echo "man ls | cat"
  echo 'echo $?'
[36-END]

[37]
NAME="exit abc"
SETUP=""
CLEAN=""
TEST=
  echo "exit abc"
  echo 'echo $?'
[37-END]

[38]
NAME="Shitty redirect"
SETUP=""
CLEAN="rm -f caca toto"
TEST=
  echo "ls > caca toto"
  echo "cat caca toto"
  echo 'echo $?'
[38-END]

[39]
NAME="echo with env inside"
SETUP=""
CLEAN=""
TEST=
  echo "echo 'ls $HOME'"
  echo 'echo $?'
[39-END]

[40]
NAME="Redirecting non existing command"
SETUP=""
CLEAN="rm -f ok"
TEST=
  echo "caca > ok"
  echo 'echo $?'
[40-END]

[41]
NAME="Triple Redirect"
SETUP=""
CLEAN="rm -f ok caca"
TEST=
  echo "ls > ok > caca"
  echo 'echo $?'
[41-END]

[43]
NAME="SIGFPE and SIGSEGV error messages"
SETUP=""
CLEAN=""
TEST=
  echo "./fpe"
  echo 'echo $?'
  echo "./sigsegv"
  echo 'echo $?'
[43-END]

[45]
NAME="Alias"
SETUP=""
CLEAN=""
TEST=
  echo "alias l ls -la"
  echo "l"
  echo 'echo $?'
[45-END]

[46]
NAME="Hard alias"
SETUP=""
CLEAN="rm -f ok"
TEST=
  echo "alias l ls -la > ok"
  echo "l"
  echo 'echo $?'
[46-END]

[47]
NAME="unalias"
SETUP=""
CLEAN=""
TEST=
  echo "alias l ls -la"
  echo "unalias l"
  echo 'echo $?'
[47-END]

[48]
NAME="Magic quotes"
SETUP=""
CLEAN=""
TEST=
  echo "ls `echo $HOME`"
  echo 'echo $?'
[48-END]

[49]
NAME="Globing"
SETUP=""
CLEAN=""
TEST=
  echo "cat src/*.c"
  echo 'echo $?'
[49-END]

[50]
NAME="Non existent bin"
SETUP=""
CLEAN=""
TEST=
  echo "CacaCacaProutProutPwetPwet"
  echo 'echo $?'
[50-END]
